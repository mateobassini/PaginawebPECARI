{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "outline: 1px solid color-mix(in oklab, var(--brand-primary) 40%, white)"
        ]
      }
    ]
  }
}